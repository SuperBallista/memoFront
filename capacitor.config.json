{
  "appId": "com.memo.memoapp",
  "appName": "MemoApp",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "android": {
    "allowMixedContent": true,
    "webContentsDebuggingEnabled": true
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 3000,
      "androidScaleType": "CENTER_CROP",
      "showSpinner": false
    },
    "CapacitorHttp": {
      "enabled": true
    }
  }
  
}
